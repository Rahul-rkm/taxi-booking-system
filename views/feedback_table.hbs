<div class="row">
    <div class="col-6">
        <h1>Feedback</h1>
    </div>
</div>

<table class="table table-dark mt-1">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Message</th>
            <th scope="col">Employee ID</th>
            <th scope="col">Customer ID</th>
            <th scope="col" class="text-end">Action</th>

        </tr>
    </thead>
    <tbody>
        {{#each rows}}
        <tr>
            <th scope="row">{{this.fdk_id}}</th>
            <th scope="row">{{this.message}}</th>
            <th scope="row">{{this.emp_id}}</th>
            <th scope="row">{{this.cust_id}}</th>
            <th scope="row" class="text-end">
                <a href="#" type="button" class="btn btn-light btn-small"><i class="bi bi-eye"> View</i></a>
            </th>
        </tr>
        {{/each}}
    </tbody>
</table>